    "@types/debug": "~4.1.7",
    "@types/tape": "~4.13.2",
    "ipfs-bitswap": "11.0.1",
    "@typescript-eslint/eslint-plugin": "~5.26.0",
    "@typescript-eslint/parser": "~5.26.0",
    "@web-std/file": "~3.0.2",
